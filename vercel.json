{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "chess-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "chess-frontend/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg))",
      "dest": "chess-frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "chess-frontend/build/index.html"
    }
  ],
  "outputDirectory": "chess-frontend/build"
}
